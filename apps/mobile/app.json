{
  "expo": {
    "name": "bomber-app",
    "slug": "bomber-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "emberdevco",
    "icon": "./assets/images/bomberappcover.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.emberdevco.bomberapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.emberdevco.bomberapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bomberappcover.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bomber-icon-blue.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bombersplash.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-linear-gradient"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
